{
  "name": "switch-tool",
  "version": "1.0.5",
  "description": "快捷切换npm镜像源以及git代理工具",
  "main": "index.js",
  "bin": {
    "sreg": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build:mac": "pkg index.js --target=node18-macos --output=myapp",
    "build:win": "pkg index.js --target=node18-win --output=myapp.exe"
  },
  "keywords": [
    "npm",
    "pnpm",
    "registry",
    "switch",
    "git"
  ],
  "author": "GYH",
  "license": "ISC",
  "dependencies": {
    "inquirer": "^12.1.0"
  }
}
